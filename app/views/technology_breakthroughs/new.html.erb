<%= form_for @technology_breakthrough, url: company_technology_breakthroughs_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %><br>

  <%= f.label :description %>
  <%= f.text_field :description, class: "form-control" %><br>

  <%= f.label :year %>
  <%= f.text_field :year, class: "form-control" %><br>

  <%= f.hidden_field :company_id, class: "form-control" %>

  <%= f.submit %>
<% end %>
