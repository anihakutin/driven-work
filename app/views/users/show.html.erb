<div class="container">
  <div class="col-md-6">
    <h2>Submitted Companies:</h2>
      <ul>
        <% @user.companies.each do |company| %>
          <li><%= link_to company.name, company_path(company) %></li>
        <% end %>
      </ul>
    <h2>Submitted Ceo's:</h2>
      <ul>
        <% @user.ceos.each do |ceo| %>
          <li><%= link_to ceo.name, ceo_path(ceo) %></li>
        <% end %>
      </ul>
  </div>
  <div class="col-md-6">
    <h2>Hello, <strong><%= @user.name %>!</strong></h2>
    <h4>User Level: <strong><%= current_user_level(@user) %></strong></h4>
    <p><%= current_user_edit_link(@user) %></p>
    <p><%= current_user_delete_link(@user) %></p>
  </div>
</div>
